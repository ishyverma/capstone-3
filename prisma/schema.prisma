generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id          String @id @default(uuid())
    firstName   String
    lastName    String
    password    String
    email       String  @unique
    createdAt DateTime  @default(now())
    updatedAt DateTime  @updatedAt
}